<div class="container">
  <div class="row">
    <div class="col-md-12 min-vh-100 d-flex flex-column justify-content-center">
      <div class="row">
        <div class="col-lg-6 col-md-8 mx-auto">
          <!-- form card login -->
          <div class="card rounded shadow shadow-sm">
            <div class="card-header">
              <h3 class="card-title">Login</h3>
            </div>
            <div class="card-body">
              <form class="form form-login">
                  <div class="form-group" style="text-align: left;">
                  <input class="form-input input-username-login" type="text" minlength="6" maxlength="20" required="required" placeholder="Enter Username"/>
                  <input class="form-input input-password-password" type="password" minlength="6" maxlength="20" required="required" placeholder="Enter Password"/>
                </div>
                  <div class="form-group">
                  <button class="button btn-primary" type="submit" id="loginButton" onclick="loginButtonClicked()">Login</button>
                   <button class="button btn-primary">
                    <a class="create-account-button btn-primary" href="/signUp">Create An Account</a>
                  </button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script  src="https://code.jquery.com/jquery.min.js"></script>
<script src="../js/login.js" type="text/javascript" ></script>



